{
  "name": "ssm-hitch",
  "version": "0.1.4",
  "author": "ssm",
  "summary": "Install and configure Hitch TLS proxy",
  "license": "Apache-2.0",
  "source": "https://github.com/ssm/ssm-hitch.git",
